{
  "name": "mubook-hon",
  "private": true,
  "keywords": [
    "notion",
    "dropbox",
    "epub",
    "book",
    "reader",
    "viewer"
  ],
  "scripts": {
    "dev": "next dev",
    "typecheck": "tsc -p .",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "test": "node --test --require=ts-node/register app/**/*.test.ts pages/api/**/*.test.ts"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.9",
    "@piotr-cz/swr-idb-cache": "^1.1.2",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-toast": "^1.2.7-rc.6",
    "@react-hookz/web": "^25.0.1",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/full-screen": "^3.12.0",
    "@react-pdf-viewer/locales": "^1.0.0",
    "dropbox": "^10.34.0",
    "exponential-backoff-generator": "^2.0.1",
    "husky": "^8.0.3",
    "msw": "^2.7.1",
    "next": "^15.1.7",
    "pdfjs-dist": "3.1.81",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-icons": "^4.9.0",
    "react-pdf": "^6.2.2",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.1.7",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  }
}
